{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "dlitem": "off",
        "definition-list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 130",
    "not ios_saf <= 18",
    "not safari <= 18"
  ]
}